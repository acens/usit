<section id="register" class="parallax-section">
	<div class="container">
		<div class="row">

			<div class="wow fadeInUp col-md-12 text-center col-sm-12" data-wow-delay="0.6s">
				<h2>Register Here</h2>
				<h3>Fill in your details and we'll contact you!</h3>
				<p>The contact will be through your phone, using a level of english appropriate to you.</p>
				</br>
			</div>

			<div class="wow fadeInUp col-md-offset-3 col-md-6 col-sm-12" data-wow-delay="1s">
				<form action="#" method="post">
					<input name="firstname" type="text" class="form-control" id="firstname" placeholder="Your Name">
					<!-- <input name="lastname" type="text" class="form-control" id="lastname" placeholder="Last Name"> -->
					<input name="phone" type="telephone" class="form-control" id="phone" placeholder="Phone Number">
					<input name="email" type="email" class="form-control" id="email" placeholder="Email Address">
					<label for="subject">Conversation level</label>
					<select class="contact[level] form-control" name="level">
							<option value="1">Basic</option>
							<option value="2">Intermediate</option>
							<option value="3">Advanced</option>
					</select>
					<textarea name="message" id="message" cols="30" rows="10" class="form-control" placeholder="Your experiences with the language"></textarea>
					<div class="col-md-offset-6 col-md-6 col-sm-offset-1 col-sm-10">
						<input name="submit" type="submit" class="form-control" id="submit" value="REGISTER">
					</div>
				</form>
			</div>

			<div class="col-md-1"></div>

